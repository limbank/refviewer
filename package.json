{
    "name": "refviewer",
    "version": "4.1.3",
    "private": true,
    "main": "main.js",
    "scripts": {
        "build:svelte": "rollup -c",
        "dev:svelte": "rollup -c -w",
        "start:svelte": "sirv public --no-clear",
        "dev:electron": "electron .",
        "dev": "concurrently \"npm run dev:electron\" \"npm run dev:svelte\"",
        "start": "concurrently \"npm run dev:electron\" \"npm run start:svelte\"",
        "postinstall": "patch-package"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "concurrently": "^6.2.0",
        "electron": "^13.1.7",
        "electron-reloader": "^1.2.1",
        "postcss": "^8.3.5",
        "postcss-import": "^14.0.2",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "sass": "^1.35.2",
        "sirv-cli": "^1.0.0",
        "svelte": "^3.49.0",
        "svelte-awesome-color-picker": "^1.2.3",
        "svelte-preprocess": "^4.7.4"
    },
    "dependencies": {
        "@panzoom/panzoom": "^4.4.4",
        "bmp-js": "^0.1.0",
        "fs-extra": "^10.1.0",
        "image-data-uri": "^2.0.1",
        "node-html-parser": "^5.3.3",
        "node-vibrant": "^3.2.1-alpha.1",
        "patch-package": "^6.4.7",
        "psd": "^3.4.0",
        "screenshot-desktop": "^1.12.7",
        "sharp": "^0.30.7",
        "svelte-canvas": "^0.7.1",
        "svelte-markdown": "^0.2.2",
        "svelte-popperjs": "^1.2.10",
        "tinycolor2": "^1.4.2"
    }
}
